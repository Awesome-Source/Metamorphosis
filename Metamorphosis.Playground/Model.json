{
  "Components": [
    {
      "Name": "Lifecycle",
      "Type": "Metamorphosis.Lifecycle"
    },
    {
      "Name": "ConsoleLogger1",
      "Type": "Metamorphosis.Logging.ConsoleLogger"
    },
    {
      "Name": "ConsoleLogger2",
      "Type": "Metamorphosis.Logging.ConsoleLogger"
    },
    {
      "Name": "TestComponent",
      "Type": "Metamorphosis.Playground.TestComponent"
    },
    {
      "Name": "SumComponent",
      "Type": "Metamorphosis.Playground.SumComponent"
    }
  ],
  "Connections": [
    {
